<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="fun.xilei.humordemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".activity.DataBindingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.FlvTestActivity"
            android:exported="true">

        </activity>
        <activity android:name=".FrescoActivity"></activity>
        <activity android:name=".kotlin.DemoActivity">

        </activity>
        <activity android:name=".activity.AActivity"
            android:configChanges="orientation|screenSize"
            >

        </activity>
        <activity android:name=".activity.BActivity"
            android:launchMode="standard">

        </activity>
        <activity android:name=".activity.CActivity"
            android:launchMode="singleTop">

        </activity>
        <activity android:name=".activity.DActivity"
            android:launchMode="singleTask">

        </activity>
        <activity android:name=".activity.EActivity"
            android:launchMode="singleInstance">

        </activity>

        <service
            android:name=".StudentService"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />

                <action android:name="fun.xilei.humordemo.StudentService" />
            </intent-filter>
        </service>

        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>